<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">


    <context:annotation-config/>

    <context:component-scan base-package="com.mogujie.service.tsharding.dao" />
    <context:component-scan base-package="com.mogujie.service.tsharding.mapper" />
    <context:component-scan base-package="com.mogujie.service.tsharding.bean" />

    <import resource="classpath*:META-INF/support/service-loader.xml"/>
    <import resource="classpath*:META-INF/support/datasource.xml"/>


</beans>